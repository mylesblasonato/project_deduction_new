#Background:enabled {
    transition-property: opacity;
    transition-duration: 0.5s;
    transition-timing-function: linear;
    opacity: 1;
}

#Background {
    opacity: 0;
    transition-property: opacity;
    transition-duration: 0.5s;
    transition-timing-function: linear;
}

#Components:enabled {
    transition-property: opacity;
    transition-duration: 0.1s;
    transition-timing-function: linear;
    opacity: 1;
}

#Components {
    opacity: 0;
    transition-property: opacity;
    transition-duration: 0.1s;
    transition-timing-function: linear;
}

#PressAnyKeyToContinueController:enabled Label {
    transition-property: font-size;
    transition-timing-function: linear;
    transition-duration: 0.2s;
    transition-delay: 1.2s;
    font-size: 60px;
    -unity-text-align: middle-center;
    color: rgb(255, 255, 255);
    -unity-font-definition: resource('Fonts/Exo2-Black');
    -unity-font-style: normal;
}

#PressAnyKeyToContinueController Label {
    font-size: 0;
    transition-duration: 0.5s;
    -unity-font-definition: resource('Fonts/Exo2-Black');
}

QuoteLabel:enabled {
    opacity: 1;
    transition-property: opacity;
    transition-duration: 0.5s;
    transition-timing-function: linear;
}

QuoteLabel {
    opacity: 0;
    transition-property: opacity;
    transition-duration: 0.5s;
    transition-timing-function: linear;
}

DisableOnEventController:disabled {
    opacity: 0;
    transition-property: opacity;
    transition-duration: 0.2s;
    transition-timing-function: linear;
    transition-delay: 1s;
}

DisableOnEventController {
    opacity: 1;
}

LinearProgressBar > #PreviewBar {
}
